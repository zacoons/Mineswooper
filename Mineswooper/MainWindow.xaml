<Window x:Class="Mineswooper.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Mineswooper"
        mc:Ignorable="d"
        Title="MainWindow" Width="450"
        Name="Minesweeper" Icon="Icon.ico" >

    <Canvas x:Name="canvas">
        <Grid x:Name="grid" />
        <Grid x:Name="overlay" Visibility="Hidden" />
        <local:GameMenu x:Name="gameMenu" Visibility="Visible" />
    </Canvas>
</Window>
